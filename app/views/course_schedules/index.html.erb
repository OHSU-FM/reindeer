<h1>Course Schedules</h1>

<table class="responsive nowrap table table-hover" id="dttb-course_schedules" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="course_schedule">Course schedule</th>
      <th data-data="start_date">Start date</th>
      <th data-data="end_date">End date</th>
      <th data-data="no_of_seats">No of seats</th>
      <th data-data="comment">Comment</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Course Schedule', new_course_schedule_path %>