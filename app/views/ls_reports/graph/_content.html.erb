<% @virtual_groups.each do |lgroup| %>
  <% if !lgroup.group_name.include? "Selecting Student" %>
    <h4 class="question-group" id="gid<%= lgroup.gid %>" style="color:green"><%= strip_tags(lgroup.group_name).html_safe %></h4>
      <% lgroup.parent_questions.each do |pquestion| %>
        <div>
          <div id="qid<%= pquestion.qid %>"></div>
            <div  class="parent-question panel panel-default">
              <% if !pquestion.question.include? "TOKEN" %>
                <div class="panel-heading">
                  <% @group_question = pquestion.question%>
                  <h4><%= strip_tags(@group_question).html_safe %></h4>
                </div>
              <% end %>

              <div class="panel-body">
                <%= render partial: "ls_reports/graph/type_#{pquestion.qtype}_question", locals: { pquestion: pquestion }%>
              </div>
            </div>
          </div>
        <% end %>

        <hr/>

      <% end %>

    <hr/>

<% end %>
