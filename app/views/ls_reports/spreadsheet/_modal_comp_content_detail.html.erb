   <% comp_type = hf_comp_type(i.to_s) %>
   <% comp_hash.each do |k, v| %>
        <% unless !k.include? comp_code %>
        <tr>  
          <% if comp_hash[k] != 0 %>

          	   	<% if comp_type == '' %>
               		<td class="align-right"><%= k %></td>
               	<% else %>
		                <td><a href="#" data-html="true" data-placement="right" rel="tooltip" <span style="white-space: normal" data-toggle="tooltip" title="<%= hf_get_comp_desc2(k + comp_type) %>" >
		                  <%= k %></a></span>
		                </td>
		            <% end %>

                <td class="align-right"><font color="purple">Level <%= i.to_s %> = <%= comp_hash[k] %></font></td>
                <% temp_detail = comp_level[k][0...-2].split("| ") %>

                <td class="align-left"> 
                    <% temp_detail.each do |t| %>
                        <% temp_t = t.sub("Final Grade and Competency Assessment", "") %>
                            <td><a data-tab-destination="<%= comp_code %>-CourseDetail^<%= temp_t %>"> <%= hf_course_desc(temp_t) %></a> </td>
                    <% end %>
                </td>
          <% end %>

        </tr>
       <% end %>
    <% end %>  