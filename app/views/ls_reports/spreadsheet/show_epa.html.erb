<%= render :partial=>'shared/modal_spinner', :locals=>{:modal_id=>'modal-report', :message=>'Loading Report'}%>
<div id="top"></div>
<div class="row">
    <div class="col-md-12 fade-in two">
        <%= render :partial=>'ls_reports/shared/navbar', :locals=>{:lime_survey=>@lime_survey}%>
    </div>
</div>
<div class="fade-in four">
    <% if @pk == "_" %>
          <% if current_user.permission_group.title.include? "Students" %>
              <%= render :partial=>'ls_reports/spreadsheet/content_epa', :locals=>{:lime_survey=>@lime_survey}%>
          <% else %>
              <%= render :partial=>'ls_reports/spreadsheet/content_epa_class_mean' %>
          <% end %>
    <% else %>
      <%= render :partial=>'ls_reports/spreadsheet/content_epa', :locals=>{:lime_survey=>@lime_survey}%>
    <% end %>
</div>
