<% if !@csl_evals.empty? %>
    <table style="width: 80%;" class="table table-condensed table-striped table-hover ls_reports_filter spreadsheet-body">
      <% @csl_evals.each do |evals| %>
        <tr bgcolor="light blue"> <th colspan="3" style="text-align:center;" ><b><%= evals.first["csl_survey"].first %> </b></th></tr>
        <tr>
          <% evals.each do |eval| %>
            <% eval.each do |key, vals| %>
              <tr>
                <% if !key.include? "csl_survey" %>
                    <td width="40%" style="text-align:right;"><%= key %></td>
                    <% vals.each do |val| %>
                      <% if val.to_s.include? "CSL Intructor: " %>
                        <td width="10%"><%= val.split(": ").last %> </td>
                      <% else %>
                        <td width="10%"><%= val %> </td>
                      <% end %>
                    <% end %>
                  </tr>
                <% end %>
            <% end %>
          <% end %>
        </tr>
      <% end %>
    </table>
<% else %>
  <h4> No CSL Narrative Feedbacks! </h4>
<% end %>
