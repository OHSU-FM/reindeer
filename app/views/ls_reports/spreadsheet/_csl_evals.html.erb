<% if !@csl_evals.nil? %>
    <table id="csl_evaluation" style="width: 80%;" class="table table-condensed table-striped table-hover ls_reports_filter spreadsheet-body center">
      <% @csl_evals.each do |evals| %>
        <tr bgcolor="light blue"> <th colspan="3" style="text-align:center;" ><b><%= evals.first["csl_survey"].first %> </b></th></tr>
        <tr>
          <% evals.each do |eval| %>
            <% eval.each do |key, vals| %>
              <tr>
                <% if !key.include? "csl_survey" %>
                    <td width="40%" style="text-align:right;"><%= key %></td>
                    <% vals.each do |val| %>
                      <% if val.to_s.include? "CSL Intructor: " %>
                        <td width="10%"><em><b><%= val.split(": ").last %> </b></em> </td>
                      <% else %>
                        <td width="10%"><em><b><%= val.html_safe %></b></em> </td>
                      <% end %>
                    <% end %>
                  </tr>
                <% end %>
            <% end %>
          <% end %>
        </tr>
      <% end %>
    </table>
<% elsif !@csl_feedbacks.nil? %>
  <%= render partial: "csl_feedbacks/csl_feedbacks", locals: {csl_feedbacks: @csl_feedbacks} %>
<% else %>
  <h4> No CSL Narrative Feedbacks! </h4>
<% end %>
