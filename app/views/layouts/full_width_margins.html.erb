<!DOCTYPE html>
<html lang="en">
<%= render :partial=>'layouts/head' %>
<body id="<%= controller_full_name %>" class="<%= current_user_theme %>" data-spy="scroll" data-target="#scroll-spy-nav">
    <%= render 'layouts/nav_header' %>

    <!-- Page Content -->
    <div id="page-content" class="container col-lg-10 col-lg-offset-1">
        <%= render 'shared/messages' %>
        <%= yield %>
    </div>

    <div class="container">

        <hr>

        <footer>
            <div class="row">
                <div class="col-lg-12">
                </div>
            </div>
        </footer>

    </div>
    <!-- /.container -->

</body>

</html>
