<!DOCTYPE html>
<html lang="en">
<%= javascript_include_tag "i18n" %>
<%= javascript_include_tag "translations", skip_pipeline: true %>
<div id="modal-holder"></div>
<%= render :partial=>'layouts/head' %>

<body id="Main" class="<%= yield(:body_classes)%>">

    <!-- Page Content -->

    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <%= content_for?(:messages) ? yield(:messages) : render('shared/messages') %>
                <%= yield %>
            </div>
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container -->

    <div class="container">
        <hr>
        <footer>
            <div class="row">
                <div class="col-lg-12">
                </div>
            </div>
        </footer>
    </div>
    <!-- /.container -->

</body>

</html>
