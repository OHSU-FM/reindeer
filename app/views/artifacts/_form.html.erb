<%= simple_form_for(@artifact) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tittle %>
    <%= f.input :content %>
  </div>

  <div class="field">
  <%= f.label :documents %>
  <%= f.file_field :documents, multiple: true %>
</div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
