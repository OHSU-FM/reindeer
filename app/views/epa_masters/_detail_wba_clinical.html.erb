<br />
<span>
  <h4 style="text-align:center"> # of WBAs by Clinical Assessor Report
  <%= link_to ('<i class="fa fa-download"></i>').html_safe, download_file_epa_masters_path(file_name: 'wba_clinical_assessor.txt') %>
  </h4>
  <h5 style="text-align:center">Cohort: <%= @cohort_title %></h5>  
</span>

<table id="wbaClinicalTable" class="table table-hover table-striped responsive" data-sort="true" >
  <thead>
    <tr class="filters">
      <% @wba_clinical_data.first.keys.each do |key| %>
        <th style="text-align:center"><%= key.titleize %></th>
      <% end %>
    </tr>
  </thead>
    <% @wba_clinical_data.each do |wba| %>
      <tr>
        <% wba.each do |key, val| %>
            <td style="text-align:center"> <%= val %> </td>
        <% end %>
      </tr>
    <% end %>
</table>
