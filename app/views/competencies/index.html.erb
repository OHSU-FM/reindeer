<div class="container-fluid">
   <ul id="MainTabs2" class="nav nav-tabs">

       <li class="bold-italic"><a data-toggle="tab" href="#fom" style="background-color:#97BC72">FoM Blocks</a></li>
       <li class="bold-italic"><a data-toggle="tab" href="#Preceptorship" style="background-color:#88B5AB">Preceptorship</a></li>
       <li class="bold-italic"><a data-toggle="tab" href="#scholarly" style="background-color:#88C7AB">Scholarly Project</a></li>
       <li class="active bold-italic"><a data-toggle="tab" href="#competency" style="background-color:#E0BBE4">Competency</a></li>
       <li class="bold-italic"><a data-toggle="tab" href="#epa" style="background-color:#FEC8F8">EPA/WBA</a></li>
       <li class="bold-italic"><a data-toggle="tab" href="#core" style="background-color:#FEC8D8">Core Courses</a></li>
       <li class="bold-italic"><a data-toggle="tab" href="#elective" style="background-color:#DAF7A6">Elective</a></li>
       <li class="bold-italic"><a data-toggle="tab" href="#nbme" style="background-color:#FAF7A6">National Board Exams/CPX</a></li>
   </ul>
  <div class="tab-content">
    <div id="fom" class="tab-pane fade">
        <%= render partial: "fom_blocks" %>
        <div class="panel panel-default">
            <div class="panel-body"><h4>CSL Evaluations</h4></div>
        </div>
         <div class="table-responsive-sm">
          <% if @csl_data.empty? %>
            <%= render partial: "csl_feedbacks/csl_feedbacks", locals: {csl_feedbacks: @csl_feedbacks} %>
          <% else %>
            <%= render partial: "csl_evals" %>
          <% end %>
        </div>

    </div>
    <div id="Preceptorship" class="tab-pane fade">
      <%= render partial: "preceptorships" %>
    </div>

    <div id="scholarly" class="tab-pane active fade">
      <h4> Coming Soon! </h4>
    </div>

    <div id="competency" class="tab-pane active fade in">
      <div class="center-div">
        <div class="inner-div">
          <% if !@chart.nil? %>
            <br /> <br />
            <%= high_chart("comp_graph", @chart) %>
            <% else %>
              <h4> No Competency/EPA data FOUND! </h4>
          <% end %>
        </div>
      </div>
    </div>
    <div id="epa" class="tab-pane active fade">
      <div class="center-div">
        <div class="inner-div">
          <% if !@chart_epa.nil? %>
            <br /> <br />
            <%= high_chart("epa_graph", @chart_epa) %>
          <% end %>
          <% wba_epa_student = hf_series_data_student("EPA", params[:user_id]) %>
          <% if !wba_epa_student.nil? %>
             <%= high_chart("wba_epa_student_graph", wba_epa_student) %>
          <% end %>
          <% wba_clinical_assessor = hf_series_data_student("Clinical Assessor", @pk) %>
          <% if !wba_clinical_assessor.nil? %>
            <%= high_chart("wba_clinical_assessor_graph", wba_clinical_assessor) %>
          <% end %>

          <%= render partial: "collapsible_wpa_details" %>

          <%= render partial: "collapsible_epas" %>
        </div>
      </div>
    </div>

    <div id="core" class="tab-pane fade">
      <div class="center-div">
          <% if !@comp.nil? %>
            <%= render partial: "core_courses" %>
          <% else %>
            <h4> No Core Courses data FOUND! </h4>
          <% end %>
        </div>
      </div>
      <div id="elective" class="tab-pane fade">
        <% if !@comp.nil? %>
          <%= render partial: "electives" %>
        <% else %>
          <h4> No Elective Courses data FOUND! </h4>
        <% end %>
      </div>

      <div id="nbme" class="tab-pane active fade">
        <% if !@comp.nil? %>
          <%= render partial: "nbme" %>
        <% else %>
          <h4> No NMBE/Shelf data FOUND! </h4>
        <% end %>
      </div>
  </div>
</div>
