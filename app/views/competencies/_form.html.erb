<br />

      <%= simple_form_for @competency, action: 'create', method: 'post' do |f| %>
              <%= f.input :user_id, collection: @users %> <br>
              <%= f.input :permission_group_id  %>
              <%= f.input :student_uid %>
              <%= f.input :email %>

              <%= f.input :medhub_id %>
              <%= f.input :course_id %>
              <%= f.input :course_name, input_html: { size: 50 }  %>
              <%= f.input :start_date, as: :date, html5: true %>
              <%= f.input :end_date, as: :date, html5: true%>
              <%= f.input :submit_date, as: :date, html5: true %>
              <%= f.input :evaluator  %>
              <%= f.input :final_grade  %>
              <%= f.input :prof_concerns, input_html: { size: 50 }  %>
              <%= f.input :mspe, input_html: { size: 20 }  %>
              <%= f.submit :submit, class: "btn btn-primary" %>

      <% end %>
