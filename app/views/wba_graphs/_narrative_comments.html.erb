
<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne">
            <h4 class="panel-title">
              <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
          Clinical Phase Comments
              </a>
            </h4>
        </div>
        <div id="collapseOne" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="headingOne">
            <div class="panel-body">
              <div class="table-responsive-sm">
                <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">CourseName</th>
                      <th scope="col">Evaluator</th>
                      <th scope="col">FinalGrade</th>
                      <th scope="col">ProfConcern</th>
                      <th scope="col">MSPE Comment</th>
                    </tr>
                  </thead>
                  <% clinical_data.each do |cd| %>
                    <tr>
                      <td><%= cd["CourseName"] %></td>
                      <td><%= cd["Eval"] %></td>
                      <td><%= cd["FinalGrade"] %></td>
                      <td><%= cd["ProfConcerns"] %></td>
                      <td><%= cd["MSPE"] %></td>
                    </tr>
                  <% end %>
                </table>
              </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingTwo">
            <h4 class="panel-title">
              <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
          Preptorship Comments
              </a>
            </h4>
        </div>
        <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
            <div class="panel-body">
              <div class="table-responsive-sm">
                <table class="table">
                  <thead>
                    <tr>
                      <th scope="col">Preceptor</th>
                      <th scope="col">SubmitDate</th>
                      <th scope="col">Grade</th>
                      <th scope="col">Comment</th>
                    </tr>
                  </thead>
                  <% preceptor_data.each do |pd| %>
                    <tr>
                      <td><%= pd["PreceptorName"] %></td>
                      <td><%= pd["SubmitDate"] %></td>
                      <td><%= pd["Grade"] %></td>
                      <td><%= pd["Comment"] %></td>
                    </tr>
                  <% end %>
                </table>
              </div>
            </div>
          </div>
      </div>
      <div class="panel panel-default">
          <div class="panel-heading" role="tab" id="headingThree">
            <h4 class="panel-title">
              <a class="collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
            CSL Narrative Feedbacks
              </a>
            </h4>
          </div>
          <div id="collapseThree" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingThree">
              <div class="panel-body"
                <div class="table-responsive-sm">
                  <table class="table">
                    <thead>
                      <tr>
                        <th scope="col">CSL Block Title</th>
                        <th scope="col">CSL Instructor</th>
                        <th scope="col">Student</th>
                        <th scope="col">Feedback</th>
                      </tr>
                    </thead>
                    <% csl_data.each do |csl| %>
                      <tr>
                        <td><%= csl["BlockName"] %></td>
                        <td><%= csl["CSLInstructor"].split(":").last %></td>
                        <td><%= csl["SelectedStudent"] %></td>
                        <td><%= csl["Comments"] %></td>
                      </tr>
                    <% end %>
                  </table>
                </div>
               </div>
           </div>
       </div>
    </div>
